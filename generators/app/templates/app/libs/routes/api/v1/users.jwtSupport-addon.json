{
  <% if (locals.includeRouteBuilder) {%>"list": {
    "method": "get",
    "url": "/",
    "secured": true
  },<%}%>
  "get": {
<% if (locals.includeRouteBuilder) {%>"method": "get",
    "url": "/:id",
    "secured": true,<%}%>
    "scheme": {
      "params": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          }
        }
      }
    }
  },
  "create": {
<% if (locals.includeRouteBuilder) {%>"method": "post",
    "url": "/",
    "secured": true,<%}%>
    "scheme": {
      "body": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "login": {
            "type": "string",
            "required": true
          },
          "password": {
            "type": "string",
            "required": true
          }
        }
      }
    }
  },
  "update": {
<% if (locals.includeRouteBuilder) {%>"method": "put",
    "url": "/:id",
    "secured": true,<%}%>
    "scheme": {
      "params": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          }
        }
      },
      "body": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "password": {
            "type": "string"
          }
        }
      }
    }
  },
  "remove": {
<% if (locals.includeRouteBuilder) {%>"method": "delete",
    "url": "/:id",
    "secured": true,<%}%>
    "scheme": {
      "params": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
          "id": {
            "type": "string"
          }
        }
      }
    }
  }
}
