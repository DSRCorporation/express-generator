{
  "express": {
    "allowAllOrigins": true,
    "serveStatic": true
  },<% if (locals.useMongo) {%>
  "mongoose": {
    "uri": "mongodb://<%= appName%>-mongo/<%=dbName%>"
  },<%}%>
  <% if (locals.useSequelize) {%>
    "sequelize" : {
      <% if (locals.usePostgres) {%>"uri": "postgres://postgres@<%= appName%>-postgres:5432/<%=dbName%>",<%}%>
      <% if (locals.useMysql) {%>"uri": "mysql://root:root_password@<%= appName%>-mysql:3306/<%=dbName%>",<%}%>
      "options": {}
  },<%}%>
  "security": {
    "jwtSecret": "<%=jwtSecret%>",
    "jwtTokenExpirationTime": 43200,
    "jwtTokenReissueTime": 3600
  }
}